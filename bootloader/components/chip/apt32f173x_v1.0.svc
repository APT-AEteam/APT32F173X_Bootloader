      <Register Name="GPTB_CEDR" Authority="RW" Address="0x40059000" Width="32" Description="ID和时钟控制寄存器">
        <Bit Name="IDCODE" Authority="RO" Bits="31-16" Description="当前EPT模块的版本信息。" />
